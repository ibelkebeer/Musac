public class Line{
  int d;
  int f;
  int j;
  int k;
  public Line(){
    d=0;
    f=0;
    j=0;
    k=0;
  }
  public void setD(int val){
    d = val;
  }
  public void setF(int val){
    f = val;
  }
  public void setJ(int val){
    j = val;
  }
  public void setK(int val){
    k = val;
  }
  public int getD(){
    return d;
  }
  public int getF(){
    return f;
  }
  public int getJ(){
    return j;
  }
  public int getK(){
    return k;
  }
  
}
